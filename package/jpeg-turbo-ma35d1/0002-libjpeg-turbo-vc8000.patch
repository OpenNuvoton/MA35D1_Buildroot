+int vc8000_v4l2_queue_output(
+	struct video *psVideo,
+	int n,
+	int length
+)
+{
+	struct video *vid = psVideo;
+
+	if (n >= vid->out_buf_cnt) {
+		fprintf(stderr, "Tried to queue a non exisiting buffer \n");
+		return -1;
+	}
+
