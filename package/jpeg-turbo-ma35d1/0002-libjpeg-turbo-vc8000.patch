+
+static double getTimeSec(void)
+{
+  struct timeval tv;
+
+  if (gettimeofday(&tv, NULL) < 0)
+	return 0.0;
+  else 
+	return (double)(tv.tv_sec ) + ((double)(tv.tv_usec / 1000000.));
+}
