+static int select_vc8k_pp_dst(nuGstVideoConvert *csp, int fbsel)
+{
+  struct vc8k_pp_params  *pp;
+  char  dev_name[32];
+  int   i;
+
+  pp = &csp->pp;
+
+  if (csp->framebuffer != NULL) {   /* close the previous opened fb0/fbb1 */
+    munmap (csp->framebuffer, csp->fixinfo.smem_len);
+    close (csp->fd);
+    csp->framebuffer = NULL;
+    csp->fd = -1;
+  }
+
+  pp->pp_out_dst = fbsel;
+
+  if (pp->pp_out_dst == PP_DST_FB1)
+    csp->device = g_strdup ("/dev/fb1");
+  else
+    csp->device = g_strdup ("/dev/fb0");
+  
+  csp->fd = open (csp->device, O_RDWR);
+
+  if (csp->fd == -1) {
+    printf("nufbdevsink open %s failed!\n", csp->device);
+    return -1;
+  }
+    printf("nufbdevsink open %s.\n", csp->device);
+
+  /* get the fixed screen info */
+  if (ioctl (csp->fd, FBIOGET_FSCREENINFO, &csp->fixinfo)) {
+    close(csp->fd);
+    csp->fd = -1;
+    return -1;
+  }
+
+  /* get the variable screen info */
+  if (ioctl (csp->fd, FBIOGET_VSCREENINFO, &csp->varinfo)) {
+    close(csp->fd);
+    csp->fd = -1;
+    return -1;
+  }
+
+  if (pp->pp_out_dst == PP_DST_FB1) {
+     if (ioctl(csp->fd, ULTRAFBIO_BLENDING_MODE, &csp->dcblend) != 0)
+       printf("ULTRAFBIO_BLENDING_MODE %d failed!\n", csp->dcblend);
+     printf("Set alpha blending mode:  %d\n", csp->dcblend);
+  }
+
+  /* map the framebuffer */
+  csp->framebuffer = mmap (0, csp->fixinfo.smem_len,
+      PROT_WRITE, MAP_SHARED, csp->fd, 0);
+  if (csp->framebuffer == MAP_FAILED) {
+    csp->framebuffer = NULL;
+    printf("nufbdevsink frame buffer mmap error!\n");
+  }
+
+  ioctl(csp->fd, FBIOPAN_DISPLAY, &csp->varinfo);
+
+  /*-----------------------------------------------*/
+  /*  VC8000 PP control                            */
+  /*-----------------------------------------------*/
+  pp->enable_pp = 1;
+  pp->frame_buf_w = csp->varinfo.xres;
+  pp->frame_buf_h = csp->varinfo.yres;
+
+  printf("frame buffer resolution is: %d x %d\n", pp->frame_buf_w, pp->frame_buf_h);
+
+  if (csp->fd_vc8k != -1) {
+    if (ioctl(csp->fd_vc8k, VC8KIOC_PP_SET_CONFIG, csp->pp) != 0)
+      printf("VC8KIOC_PP_SET_CONFIG failed (%s)", strerror(errno));
+    return 0;
+  }
+
+  /*
+   * First itme to init VC8K
+   */
+  pp->img_out_x = 0;
+  pp->img_out_y = 0;
+  pp->img_out_w = csp->varinfo.xres;
+  pp->img_out_h = csp->varinfo.yres;
+  pp->rotation  = PP_ROTATION_NONE;
+  pp->img_out_fmt = V4L2_PIX_FMT_ARGB32;
+
+  for (i = 0; i < 10; i++) {
+    sprintf(dev_name, "/dev/video%d", i);
+    csp->fd_vc8k = open(dev_name, O_RDWR);
+    if (csp->fd_vc8k == -1)
+      continue;
+
+    if (ioctl(csp->fd_vc8k, VC8KIOC_PP_SET_CONFIG, csp->pp) != 0) {
+      // printf("VC8KIOC_PP_SET_CONFIG failed (%s)", strerror(errno));  // not an error
+      continue;
+    }
+    else
+      break;
+  }
+  if (i < 10) {
+    printf("VC8000 %s opened.\n", dev_name);
+    return 0;
+  }
+  return -1;
+  }
